@{
    Layout = "_Layout";
    ViewBag.Title = "Home Page";
}
<!-- ONE-STOP -->
<div class="w-90 p-0 bg-secondary text-center one-stop ">
    <div class="card text-secondary border-0 rounded-0 h-100 bg-thirdly">
        <div class="card-body text-center">
            <a> <h5 class="card-title">ONE-STOP</h5></a>
            <div class="offer-divider center"></div>
            <p class="card-text">For kids who are struggling with scores and confidence and have slipped behind. We guarantee results!</p>
        </div>
    </div>
</div>
<header class="container-fluid bg-primary text-secondary text-center ">
    <div class="row mx-auto">

        <div class="col-xl-9 col-md-12 col-xs-12 p-0">
            <div id="main-carousel" class="carousel slide forceFullHeight carousel-fade" data-ride="carousel">
                <ol class="carousel-indicators">
                    <li data-target="#main-carousel" data-slide-to="0" class="active"></li>
                    <li data-target="#main-carousel" data-slide-to="1"></li>
                    <li data-target="#main-carousel" data-slide-to="2"></li>
                    <li data-target="#main-carousel" data-slide-to="3"></li>
                </ol><!-- /.carousel-indicators -->

                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <img class="d-block w-100 img-fluid resizable-img carousel-img" src="~/images/1.png" alt="">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100 img-fluid resizable-img carousel-img" src="~/images/2.png" alt="">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100 img-fluid resizable-img carousel-img" src="~/images/3.png" alt="">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100 img-fluid resizable-img carousel-img" src="~/images/4.png" alt="">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100 img-fluid resizable-img carousel-img" src="~/images/5.png" alt="">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100 img-fluid resizable-img carousel-img" src="~/images/6.png" alt="">
                    </div>
                </div><!-- /.carousel-inner -->

                <a href="#main-carousel" class="carousel-control-prev" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon"></span>
                    <span class="sr-only" aria-hidden="true">Prev</span>
                </a>
                <a href="#main-carousel" class="carousel-control-next" role="button" data-slide="next">
                    <span class="carousel-control-next-icon"></span>
                    <span class="sr-only" aria-hidden="true">Next</span>
                </a>
            </div><!-- /.carousel -->
        </div>

        <!--Subject offers-->
        <div class="col-xl-3 col-lg-12 col-md-12 col-xs-12 float-right no-padding offers" id="offers">
            <div class="card border-0 rounded-0 first offer">
                <div class="card-body card-txt d-flex flex-column justify-content-around">
                    <div class="">
                        <h5 class="card-title text-uppercase">English learning</h5>
                        <p class="card-text">For kids who are struggling with scores and confidence and have slipped behind. We guarantee results!</p>
                        <a href="#" class="btn btn-primary">Learn more</a>
                    </div>
                </div>
                <div class="offer-divider"></div>
            </div>

            <div class="card border-0 rounded-0 second offer">
                <div class="card-body card-txt align-content-center d-flex flex-column justify-content-around">
                    <div class="">
                        <h5 class="card-title text-uppercase">Math & Science</h5>
                        <p class="card-text">For the smartest</p>
                        <a href="#" class="btn btn-primary">Learn more</a>
                    </div>
                </div>
                <div class="offer-divider"></div>
            </div>

            <div class="card border-0 rounded-0 third offer">
                <div class="card-body card-txt d-flex flex-column justify-content-around">
                    <div>
                        <h5 class="card-title text-uppercase">Credit & School application</h5>
                        <p class="card-text">Helping you with school and credit applications</p>
                        <a href="#" class="btn btn-primary">Learn more</a>
                    </div>
                </div>
                <div class="offer-divider"></div>
            </div>

            <div class="card border-0 rounded-0 fourth offer">
                <div class="card-body card-txt d-flex flex-column justify-content-around">
                    <div>
                        <h5 class="card-title text-uppercase">Test prep</h5>
                        <p class="card-text">For kids who are on the fast track or need help with advanced coursework, homework and big tests.</p>
                        <a href="#" class="btn btn-primary">Learn more</a>
                    </div>
                </div>
            </div>
        </div>

    </div>
</header>

